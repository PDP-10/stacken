*FT.CTY=0				;DON'T SUPPORT "CONSOLE TTY"
*FT.PFH=1				;SUPPORT "PREFERRED HOST" SELECTION
*    CTYPFH=26				;  CTY PREFERS KL1026
*    TTYPFH=26				;  TTY PREFERS KL1026
*FTDZ11=1
*.MACRO DZCNFG				;DEFINE DZ-11 LINE USAGE
*APTPN=0				;NUMBER OF DH PTP DDCMP LINES
*ARDAN=0
*;DZSET	01,PTP,9600.,9600.		;DZ 0 LINE 1 = PTP DDCMP @ 9600
*
*TTYN=10-APTPN				;NUMBER OF DZ TERMINAL LINES
*TD=	0				;DZ 0 LINE 0  =  DWARF_TTY0
;000
*TDEF	TD,<<RS,9600.>,<XS,9600.>>		;LINK TO DTE
*TDEF	TD,<<PTP>,<RS,9600.>,<XS,9600.>>	;ASYNCH PTP DDCMP FOR TESTING
*TDEF	TD,<<RS,9600.>,<XS,9600.>>		;VIDEO TERMINAL
*TDEF	TD,<<RS,9600.>,<XS,9600.>>		;VIDEO TERMINAL
*TDEF	TD,<<RS,9600.>,<XS,9600.>>		;VIDEO TERMINAL
*TDEF	TD,<<RS,9600.>,<XS,9600.>>		;VIDEO TERMINAL
*TDEF	TD,<<RS,9600.>,<XS,9600.>>		;VIDEO TERMINAL
*TDEF	TD,<<RS,9600.>,<XS,9600.>>		;VIDEO TERMINAL
*
*DZUSE	TTYN,APTPN,0,0,0,ARDAN
*.ENDM
