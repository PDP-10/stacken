.R TECO
*EWCN8731.P11
*IOURNNM=31
*.MACRO	NODE	MNAME
*	MNAME	<NOVA>
*.ENDM
*FT.87S=1
*MSGMAX=596.
*NGHMAX=8.
*SCBMAX=32.
*DQN=2
*DMCN=1
*DUPN=1
*    DMCIBF=3				;SHORT OF MEMORY SPACE
*    DMCOBF=3				;SHORT OF MEMORY SPACE
*    L1LVL=06.				;HIGH-SPEED DMR (250Kb)
*NLINES=DQN+DMCN+DUPN
*FT.DDP=1				;ENABLE "DDP" SUPPORT
*    DDPPFH=26				;BY DEFAULT GO TO KL1026
*DEBUG=1
*FT.PAT=100				;SOME PATCH SPACE
*FTWDDL=1
*FTECHO=1
*    ECHON=2
*LP11N=1				;ONE LP11-BASED LINEPRINTER
*    LP0DVU=1				;LP05-CLASS PRINTER (LP14 IN ACTUALITY)
*    LP0.LC=1				;AND IT IS LOWER-CASE
;
;		***   	Async line configuration for node NOVA   ***
;
;	This configration runs 1 async Point-To-Point DDCMP communications
;	line using DH 0 line 1.
;
*FTHOST=1				;SUPPORT TERMINAL "SET HOST" CAPABILITY
*FT.CTY=0				;DON'T SUPPORT "CONSOLE TTY"
*FTDM11=1				;SUPPORT "DH11/DM11" DATASET
*FTDN11=1				;GENERATE DIALOUT CODE
*    DN11N=1				;ONE DN11 (WITH TWO UNITS)
*FT.PFH=1				;SUPPORT "PREFERRED HOST" SELECTION
*    CTYPFH=26				;  CTY PREFERS KL1026
*    TTYPFH=26				;  TTY PREFERS KL1026
*.MACRO DHCNFG				;DEFINE DH11 LINE USAGE
*APTPN=0				;NUMBER OF ASYNC DH PTP DDCMP LINES
*; DHSET	01,PTP,9600.,9600.		;DH 0 LINE 01 = PTP DDCMP @ 9600
*
*TTYN=20-APTPN				;NUMBER OF ASYNC DH TERMINAL LINES
*TD=	0				;DH 0 LINE 0  =  NOVA_TTY0
;000
*TDEF	TD,<<RS,9600.>,<XS,9600.>>		;LINK TO DTE
*TDEF	TD,<<RS,9600.>,<XS,9600.>>		;ASYNCH PTP DDCMP LINK
*TDEF	TD,<<RS,2400.>,<XS,2400.>,<WID,120.>>	;LA120 TERMINAL
*TDEF	TD,<<DSL>>
*TDEF	TD,<<DSL>>
*TDEF	TD,<<DSL>>
*TDEF	TD,<<DSL>>
*TDEF	TD,<<DSL>>
;010
*TDEF	TD,<<DSL>,<801,0>,<RS,300.>,<XS,300.>>	;DN11 DATASET
*TDEF	TD,<<DSL>,<801,1>,<RS,300.>,<XS,300.>>	;DN11 DATASET
*TDEF	TD,<<DSL>>
*TDEF	TD,<<DSL>>
*TDEF	TD,<<DSL>>
*TDEF	TD,<<DSL>>
*TDEF	TD,<<DSL>>
*TDEF	TD,<<DSL>>
*
*DHUSE	TTYN,APTPN,0,0,0,0
*.ENDM
*12II
*.NLIST
*EX
.R MACDLX
*DN8731,DN8731.CRF/CRF=CN8731,S,MACROS,DNCNFG,DNCOMM,DNDEV,DNNCL,DNDCMP,DNDTE,DNCDDQ,DNCDUP,DNCDMC,DNTTY,DNDH11,DNDN11,DNCTAB,DNLPT,DNDBG,DNLBLK,CHK11
.R DDT11
*DN8731.CRF/SYMBOL
.SSAVE DN8731
.DELETE CN8731.P11,DN8731.CRF
